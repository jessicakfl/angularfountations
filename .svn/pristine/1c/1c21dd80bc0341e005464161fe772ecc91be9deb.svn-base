.example-box {
    /* width: 200px; */
    /* width: inherit; */
    min-width: 5px;
    /* height: 200px; */
    height: auto;
    min-height: 50px;
    border: solid 1px #ccc;
    color: rgba(0, 0, 0, 0.87);
    cursor: move;
    display: inline-flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    background: #fff;
    border-radius: 4px;
    position: relative;
    z-index: 1;
    transition: box-shadow 200ms cubic-bezier(0, 0, 0.2, 1);
    box-shadow: 0 3px 1px -2px rgba(0, 0, 0, 0.2),
                0 2px 2px 0 rgba(56, 43, 43, 0.14),
                0 1px 5px 0 rgba(0, 0, 0, 0.12);
  }
  
  .example-box:active {
    box-shadow: 0 5px 5px -3px rgba(0, 0, 0, 0.2),
                0 8px 10px 1px rgba(0, 0, 0, 0.14),
                0 3px 14px 2px rgba(0, 0, 0, 0.12);
  }
  
  .example-container {
    /* width: 400px; */
    /* width: 33%; */
    
    width: 33%;
    /* min-width: 15px; */
    max-width: 100%;
    /* margin: 0 25px 25px 0; */
    /* margin: 0.1% 0.1% 0.1% 0.1%; */
    /* margin: inherit; */
    display: inline-block;
    vertical-align: top;
    margin: 1px;
  }
  
  .example-list {
    border: none;
    /* border: solid 1px #ccc; */
    min-height: 60px;
    background: rgb(248, 248, 248);
    /* background: rgb(243, 243, 243); */
    border-radius: 4px;
    overflow: hidden;
    display: block;
  }
  
  .example-list-to-delete {
    min-height: 20px;
    /* max-height: 500px; */
    min-width: 20px;
    /* max-width: 20px; */
    border: dashed 2px red;
    background: rgb(221, 170, 170);
  }

  .example-box {
    /* padding: 20px 10px; */
    /* margin: 0% 0.5% 0.5% 0.5%; */
    
    margin: 0% 4px 5px 4px;
    
    /* border-bottom: solid 1px #ccc; */
    border-bottom: solid 1.5px #000080;
    border-left: solid 1.5px #000080;
    border-right: solid 1.5px #000080;
    /* border-bottom: solid 2px #7676AC;
    border-left: solid 2px #7676AC;
    border-right: solid 2px #7676AC; */
    color: rgba(0, 0, 0, 0.87);
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    /* box-sizing: content-box; */
    /* width: 100%; */
    cursor: move;
    background: white;
    font-size: 14px;
    /* border-radius: 4px; */
  }
  
  .cdk-drag-preview {
    box-sizing: border-box;
    border-radius: 4px;
    box-shadow: 0 5px 5px -3px rgba(0, 0, 0, 0.2),
                0 8px 10px 1px rgba(0, 0, 0, 0.14),
                0 3px 14px 2px rgba(0, 0, 0, 0.12);
  }
  
  .cdk-drag-placeholder {
    opacity: 0;
  }
  
  .cdk-drag-animating {
    transition: transform 250ms cubic-bezier(0, 0, 0.2, 1);
  }
  
  /* .example-box:last-child {
    border: none;
  } */
  
  .example-list.cdk-drop-list-dragging .example-box:not(.cdk-drag-placeholder) {
    transition: transform 250ms cubic-bezier(0, 0, 0.2, 1);
  }


  /* .example-container:empty {
    display: none;
  } */



/**********************
** style for table: 
***********************/

table, th, td {
    /* border: 1px solid black; */
    background-color: gainsboro;
    border: 1px solid gainsboro;
    /* border: 1px solid white; */
}
table {
    width: 100%;
}

td {
    vertical-align: top;
    background-color: gainsboro;
}


/**********************
** style for mat-table: 
***********************/



  table {
    width: 100%;
    /* padding:0.8ex; */
    /* margin:0.5ex; */
}

.mat-row {
    height: auto;
  }
  
  .mat-cell {
    padding: 1.5px 1.5px 1.5px 0;
  }

  th {
    /* display: table-cell;
    vertical-align: inherit; */
    font-weight: bold;
    font: 16px;
    background-color:lightsteelblue;
    font: 14px!important;
  }
  tr {
    font-size: 12px!important;
  }

  /* Table row: background-color of every even row: */

  tr:nth-child(even) {background-color: #FFF8DC;}

  .mat-header-cell,  .mat-header-row {
    font-size: 13px;
    height:2em;
    min-width: 1rem;
  }
  
  .mat-header-cell {
    display:table-cell;
    padding:0;
    font-weight:bold;
    color:rgba(0, 0, 0, 0.87)
  }

  /* Table row background-color */
  .mat-row {
    background-color: #FFF1B7;
  }

  .mat-header-row {
    background-color:rgb(190, 190, 250);
    /* initial color: */
    /* background-color:lightsteelblue; */
  }
  
  .mat-cell {
    font-size: 12px;
    display: table-cell;
  }

  .mat-table {
    width:100%;
    height:100%;
  }
  
  .example-box {
    text-align:left;
    align-items:flex-start;
  }


  /* Custom Style for Table Caption(s) */

  caption.myTitleCaption {
    caption-side: top;
    background-color:rgb(190, 190, 250);
    /* padding:0.8ex; */
    /* padding:0; */
    font-weight: bold;
    border: 1px solid gainsboro;
    /* display: -moz-grid-line; */
    width: auto;
    height: 20px;
    text-align: left;
    /* color:green; */
    /* justify-content: flex-end; */
    /* display: -moz-grid-line; */
    /* margin: 0; */
    text-align: left;
    vertical-align: top;
    vertical-align: text-top;
    /* padding: 0px 0px 6px 0px; */
  }

  caption.myFooter1Caption {
    caption-side: bottom;
    color: white;
    background-color:rgb(118, 118, 172);
    /* padding:0.8ex; */
    padding:0;
    font-weight: normal;
    /* border: 1px solid gainsboro; */
    display: -moz-grid-line;
    width: auto;
    text-align: left;
    border-top: solid 1px #000080;
    /* cursor: pointer; */
  }

  caption.myFooter2Caption {
    caption-side: bottom;
    color: white;
    background-color:rgb(118, 118, 172);
    /* padding:0.8ex; */
    padding:0;
    font-weight: normal;
    /* border: 1px solid gainsboro; */
    display: -moz-grid-line;
    width: auto;
    text-align: left;
    border-top: solid 1px #000080;
  }


/* Custom Style for column "Qty" */

.mat-column-Qty {
    text-align: right;
}


/* Style for icon "close" in Delete Box */

.icon-close-tab-red {
  background-repeat: no-repeat;
  fill: currentColor;
  height: 12px;
  width: 12px;
  font-size: 14px !important;
  font-weight: 700;
  size:10px; 
  float: right;
  color:red;
}


