
<div class="entrance">
    <table>
        <!-- <tr><h3>Tables Configurations</h3></tr> -->
        <tr>
            <td>
                <h3>Image Paging</h3>
            </td>
            <td> <input type="checkbox" #ifimagepagingbox id="ifimagepagingbox" [checked]="ifimagepaging"
                    (change)="onSaveIfImagePaging(ifimagepagingbox.checked)"></td>
        </tr>
        <tr>
            <td>
                <h3>
                    Imagedirection Paging</h3>
            </td>
            <td> <input type="checkbox" #ifimagedirectionpagingbox id="ifimagedirectionpagingbox"
                    [checked]="ifdirectionpaging"
                    (change)="onSaveIfImageDirectionPaging(ifimagedirectionpagingbox.checked)"></td>
        </tr>

        <tr>
            <td>
                <h3>
                    Imagenotes Paging</h3>
            </td>
            <td> <input type="checkbox" #ifimagenotepagingbox id="ifimagenotepagingbox" [checked]="ifnotepaging"
                    (change)="onSaveIfImageNotePaging(ifimagenotepagingbox.checked)"></td>
        </tr>
    </table>
</div>