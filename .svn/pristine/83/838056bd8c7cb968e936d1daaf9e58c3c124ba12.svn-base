.example-box {
    min-width: 5px;
    height: auto;
    min-height: 50px;
    border: solid 1px #ccc;
    color: rgba(0, 0, 0, 0.87);
    cursor: move;
    border-radius: 4px;
    position: relative;
    z-index: 1;
    transition: box-shadow 200ms cubic-bezier(0, 0, 0.2, 1);
    box-shadow: 0 3px 1px -2px rgba(0, 0, 0, 0.2),
                0 2px 2px 0 rgba(56, 43, 43, 0.14),
                0 1px 5px 0 rgba(0, 0, 0, 0.12);
    margin: 0% 4px 5px 4px;
    border-bottom: solid 1.5px #000080;
    border-left: solid 1.5px #000080;
    border-right: solid 1.5px #000080;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    box-sizing: border-box;
    background: white;
    font-size: 14px;  
    text-align: left;
    align-items: flex-start;
  }
  
  .example-box:active {
    box-shadow: 0 5px 5px -3px rgba(0, 0, 0, 0.2),
                0 8px 10px 1px rgba(0, 0, 0, 0.14),
                0 3px 14px 2px rgba(0, 0, 0, 0.12);
  }
  
  .example-container {
    width: 33%;
    max-width: 100%;
    display: inline-block;
    vertical-align: top;
    margin: 1px;
  }
  
  .example-list {
    border: none;
    min-height: 60px;
    background: rgb(248, 248, 248);
    border-radius: 4px;
    overflow: hidden;
    display: block;
  }
  
  .example-list-to-delete {
    min-height: 20px;
    min-width: 20px;
    border: dashed 2px red;
    background: rgb(221, 170, 170);
  }
  
  .cdk-drag-preview {
    box-sizing: border-box;
    border-radius: 4px;
    box-shadow: 0 5px 5px -3px rgba(0, 0, 0, 0.2),
                0 8px 10px 1px rgba(0, 0, 0, 0.14),
                0 3px 14px 2px rgba(0, 0, 0, 0.12);
  }
  
  .cdk-drag-placeholder {
    opacity: 0;
  }
  
  .cdk-drag-animating {
    transition: transform 250ms cubic-bezier(0, 0, 0.2, 1);
  }
  
  .example-list.cdk-drop-list-dragging .example-box:not(.cdk-drag-placeholder) {
    transition: transform 250ms cubic-bezier(0, 0, 0.2, 1);
  }


/**********************
** Style for Fourth Column, containing the Delete Box:
***********************/

#fourthcolumn {
  width: 20px;
}

table {
    width: 100%;
}

td {
    vertical-align: top;
}

th {
  font-weight: bold;
  background-color: lightsteelblue;
  font: 14px;
}

tr {
  font-size: 12px!important;
}

/* Table row: background-color of every even row: */
tr:nth-child(even) {
  background-color: #FFF8DC;
}



/**********************
** style for mat-table: 
***********************/

  .mat-row {
    height: auto;
  }
  
  .mat-cell {
    padding: 1.5px 1.5px 1.5px 0;
    font-size: 12px;
    display: table-cell;
  }

  .mat-header-cell,  .mat-header-row {
    font-size: 13px;
    height: 2em;
    min-width: 1rem;
  }
  
  .mat-header-cell {
    display: table-cell;
    padding: 0;
    font-weight: bold;
    color: rgba(0, 0, 0, 0.87)
  }

  /* Table row background-color */
  .mat-row {
    background-color: #FFF1B7;
  }

  .mat-header-row {
    background-color: #d8d8fc;
  }

  .mat-table {
    width: 100%;
    height: 100%;
  }


/**********************
** Custom Style for Table Caption(s)
***********************/

  caption.myTitleCaption {
    caption-side: top;
    background-color: rgb(190, 190, 250);
    font-weight: bold;
    border: 1px solid gainsboro;
    width: auto;
    height: 20px;
    text-align: left;
    vertical-align: top;
    vertical-align: text-top;
  }

  caption.myFooter1Caption {
    caption-side: bottom;
    color: white;
    background-color: rgb(118, 118, 172);
    padding: 0;
    font-weight: normal;
    display: -moz-grid-line;
    width: auto;
    text-align: left;
    border-top: solid 1px #000080;
  }

  caption.myFooter2Caption {
    caption-side: bottom;
    color: white;
    background-color:rgb(118, 118, 172);
    padding:0;
    font-weight: normal;
    display: -moz-grid-line;
    width: auto;
    text-align: left;
    border-top: solid 1px #000080;
  }



/* Custom Style for column "Qty" */
  .mat-column-Qty {
    text-align: right;
  }


/* Style for icon "close" in Delete Box */
.icon-close-tab-red {
  background-repeat: no-repeat;
  fill: currentColor;
  height: 12px;
  width: 12px;
  font-size: 14px !important;
  font-weight: 700;
  size: 10px; 
  float: right;
  color: red;
}


/* <button mat-icon-button class="example-icon" routerLink="mytable">EMIS</button> */
.resize-action[_ngcontent-fsy-c95] {
  position: absolute;
  left: 100%;
  top: 100%;
  transform: translate3d(-50%, -50%, 0) rotateZ(45deg);
  border-style: solid;
  border-width: 8px;
  border-color: transparent transparent transparent orange;
}



/**********************
** Style for collapsable (expansion) panel:
***********************/

.collapsable-panel-box {
  margin: 0.0em; 
  width: inherit; 
}

.mat-expansion-panel {
  background-color: #FFF8DC;
  border-radius: 0 !important;
  padding: 0px !important;
}

.mat-expansion-panel-header {
  background-color: #ee9139 !important;
  border-radius: 0;
  height: 1.0em;
  padding: 0.15em .7em 0.2em 0em;
}

.mat-expansion-panel-header-title {
  font-size: 13px;
  margin-left: 16px;
  padding-top: 5px;
}

.collapsable-panel-headers-align .mat-expansion-panel-header-title,
.collapsable-panel-headers-align .mat-expansion-panel-header-description {
  flex-basis: 0;
}

.collapsable-panel-headers-align .mat-expansion-panel-header-description {
  justify-content: space-between;
  align-items: center;
}

/* inline list of modules: */
.collapsable-panel-box ul {
  padding-inline-start: 0px;
}

/* list item of list of modules: */
.collapsable-panel-box li {
  display: inline-block;
}

/* Button '+' to add module: */
.collapsable-panel-box button {
  color: green;
  background-color: gainsboro;
}

.module-left-side-info {
  display: table-column; 
  float: left;
}

.module-right-side-button-add {
  display: table-column;
  float: right;
}

.module-title {
  font-size: 13px;
  padding-left: 5px;
  font-weight: 450;
}

.module-avg-load {
  font-size: 11px;
  padding-left: 5px;
}

.div-module-available-to-add {
  width: 250px;
  height: 40px;
  border-radius: 4px;
  margin: 10px;
  background-color: gainsboro;
}

.div-module-already-added {
  width: 250px;
  height: 40px;
  border-radius: 4px;
  margin: 10px;
  background-color: #fccfd7;
}
